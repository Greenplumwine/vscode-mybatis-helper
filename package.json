{
  "name": "mybatis-helper-vscode",
  "displayName": "Mybatis Helper",
  "description": "%description%",
  "version": "0.0.3",
  "publisher": "Greenplumwine",
  "icon": "static/icon/mybatis-helper-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Greenplumwine/vscode-mybatis-helper.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mybatis-helper.toggleLogInterceptor",
        "title": "%command.toggleLogInterceptor.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.showSqlOutput",
        "title": "%command.showSqlOutput.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.clearSqlHistory",
        "title": "%command.clearSqlHistory.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.jumpToXml",
        "title": "%command.jumpToXml.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.jumpToMapper",
        "title": "%command.jumpToMapper.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.refreshMappings",
        "title": "%command.refreshMappings.title%",
        "category": "MyBatis Helper"
      },
      {
        "command": "mybatis-helper.refreshData",
        "title": "%command.refreshData.title%",
        "category": "MyBatis Helper"
      }
    ],
    "keybindings": [
      {
        "command": "mybatis-helper.jumpToXml",
        "key": "alt+x",
        "when": "editorFocus"
      },
      {
        "command": "mybatis-helper.jumpToMapper",
        "key": "alt+m",
        "when": "editorFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "mybatis-helper.jumpToXml",
          "group": "navigation@1"
        },
        {
          "when": "editorFocus",
          "command": "mybatis-helper.jumpToMapper",
          "group": "navigation@2"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "MyBatis Helper",
      "properties": {
        "mybatis-helper.databaseType": {
          "type": "string",
          "default": "mysql",
          "description": "Database type for SQL formatting (mysql, oracle, sqlserver, postgresql)"
        },
        "mybatis-helper.enableLogInterceptor": {
          "type": "boolean",
          "default": true,
          "description": "Enable SQL log interceptor"
        },
        "mybatis-helper.enableCodeLens": {
          "type": "boolean",
          "default": true,
          "description": "Enable CodeLens for jumping between Mapper and XML files"
        },
        "mybatis-helper.fileOpenMode": {
          "type": "string",
          "enum": [
            "useExisting",
            "noSplit"
          ],
          "default": "useExisting",
          "description": "File open mode"
        },
        "mybatis-helper.customXmlDirectories": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Custom XML directories to search for mapper files, relative to workspace root"
        },
        "mybatis-helper.logOutputLevel": {
          "type": "string",
          "enum": [
            "info",
            "debug"
          ],
          "default": "info",
          "description": "Log output level"
        },
        "mybatis-helper.enablePerformanceTracking": {
          "type": "boolean",
          "default": false,
          "description": "Enable performance tracking"
        },
        "mybatis-helper.sqlFormatOptions": {
          "type": "object",
          "default": {},
          "description": "SQL format options"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
